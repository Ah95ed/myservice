# Firebase Admin SDK credentials (path to service account JSON)
GOOGLE_APPLICATION_CREDENTIALS=path\to\service-account.json

# Firebase Realtime Database URL
FIREBASE_DATABASE_URL=https://your-project-id.firebaseio.com

# Cloudflare migration endpoint base (worker URL)
CLOUDFLARE_API_BASE_URL=https://your-worker.your-domain.workers.dev

# Cloudflare migration token (must match worker secret MIGRATION_TOKEN)
CLOUDFLARE_MIGRATION_TOKEN=replace_with_strong_token
